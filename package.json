  "private": true,
  "scripts": {
    "ng": "ng",
    "dev": "./php_wrap.sh",
    "start": "echo 'ERROR: Use `npm run dev` for development. For deployment, use the `dist` directory.'",
    "build": "ng build --prod",
    "postbuild": "./php_dist.sh; rm dist/drax-config.dev.json",
    "postinstall": "cd php/auth; ./install.sh;",
    "deploy": "npx shipit default deploy; rm -rf ./tmp",
    "deploy:dev": "npx shipit dev deploy; rm -rf ./tmp",
    "ci": "ng test --browsers ChromeHeadless,FirefoxHeadless --single-run",
    "test": "ng test",
    "test:nomap": "ng test --sourcemap=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
